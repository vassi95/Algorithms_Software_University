
import java.util.List;

public class Insertion_Sort {

	public static void sort(int[] arr){
		for(int i=1; i<arr.length; i++){
			while(i>0 && arr[i]> arr[i-1]){
				int temp =arr[i];
				arr[i] =  arr[i-1];
				arr[i-1] = temp;
				i--;
			}
		}
		
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
